<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE extentreports>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>DARK</theme>

        <!-- enables timeline -->
        <!-- defaults to true -->
        <enableTimeline>true</enableTimeline>

         <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

         <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        
        <enableOfflineMode>true</enableOfflineMode>

        <!-- title of the document -->
        <documentTitle>Relatório dos Testes</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>RELATÓRIO DOS TESTES</reportName>
        <timeStampFormat>dd/MM/yyyy HH:mm:ss</timeStampFormat>
        
        <!-- custom javascript -->
        <js>
            <![CDATA[
            ]]>
        </js>

        <!-- custom styles -->
        <css>
            <![CDATA[
            ]]>
        </css>
    </configuration>
</extentreports>